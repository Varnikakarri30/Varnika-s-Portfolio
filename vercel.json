{
  "version": 2,
  "builds": [
    {
      "src": "html/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/videos/(.*)",
      "dest": "/videos/$1"
    },
    {
      "src": "/documents/(.*)",
      "dest": "/documents/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/html/$1"
    },
    {
      "src": "/",
      "dest": "/html/index.html"
    }
  ]
}

