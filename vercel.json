{
  "version": 2,
  "rewrites": [
    {
      "source": "/css/:path*",
      "destination": "/css/:path*"
    },
    {
      "source": "/js/:path*",
      "destination": "/js/:path*"
    },
    {
      "source": "/images/:path*",
      "destination": "/images/:path*"
    },
    {
      "source": "/videos/:path*",
      "destination": "/videos/:path*"
    },
    {
      "source": "/documents/:path*",
      "destination": "/documents/:path*"
    },
    {
      "source": "/html/:path*",
      "destination": "/html/:path*"
    },
    {
      "source": "/",
      "destination": "/html/index.html"
    }
  ]
}
